@{
}

@model Mission2.Models.GradecalcModel; 

<h2>Grade Calculator: </h2>
<form method="post">
    <label>Assignments:</label>
    <input id="assignment" asp-for="Assignment" /><br />
    <label>Group Project:</label>
    <input id="project" asp-for="Project" /><br />
    <label>Quizzes:</label>
    <input id="quiz" asp-for="Quiz" /><br />
    <label>Exams:</label>
    <input id="exam" asp-for="Exam"/><br />
    <label>INTEX:</label>
    <input id="intex" asp-for="Intex" /><br /><br />
    <input id="gradecalc" type="submit" value="Calculate" />

    <div asp-validation-summary="All"></div>
</form>



<!--Brings in JavaScript page-->
<script src="~/js/grade_calc.js"></script>



